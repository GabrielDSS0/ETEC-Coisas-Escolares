		valorTotal3 = valorProduto3 * quantidadeProduto3
		valorTotal4 = valorProduto4 * quantidadeProduto4
		valorTotal5 = valorProduto5 * quantidadeProduto5

		escreva("Primeiro produto: ", "\nValor: ", valorProduto1, "\nQuantidade:", quantidadeProduto1, "Valor total a pagar: ", valorTotal1)
