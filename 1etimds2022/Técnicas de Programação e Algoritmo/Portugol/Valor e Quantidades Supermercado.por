	funcao inicio()
	{
		real valorProduto1, valorProduto2, valorProduto3, valorProduto4, valorProduto5,
		quantidadeProduto1, quantidadeProduto2, quantidadeProduto3, quantidadeProduto4, quantidadeProduto5,
		valorTotal1, valorTotal2, valorTotal3, valorTotal4, valorTotal5

		escreva("Digite o valor do primeiro produto: ")
